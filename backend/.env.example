# LLM Configuration
GEMINI_API_KEY=your-gemini-api-key-here
MODEL=gemini-2.5-flash

# API Configuration
API_KEY_SALT=random-salt-string-here
RATE_LIMIT=100

# Environment
ENVIRONMENT=development

# Database (optional)
# DATABASE_URL=postgresql://user:pass@localhost:5432/ads_quality_rater

# Redis (optional)
# REDIS_URL=redis://localhost:6379

# Monitoring
LOG_LEVEL=INFO

# Disable CrewAI Telemetry (prevents timeout to telemetry.crewai.com)
OTEL_SDK_DISABLED=true

# Google Cloud (for production)
# GOOGLE_CLOUD_PROJECT=your-project-id
